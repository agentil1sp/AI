{
    "name": "AI",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["popup.html"],
        "matches": ["http://*/*", "https://*/*"],
        "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
      }
    ]
  }